@model List<ResultContactDto>

<div class="contact section">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4">
                <div class="section-heading text-center">
                    <h6>| Bize Ulaşın</h6>
                    <h2>Bağlantı Kurun: Bizimle İletişime Geçin ve Hayalinizdeki Ev İçin Adım Atın</h2>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="contact-content">
    <div class="container">
        <div class="row">
            <div class="col-lg-7">
                @foreach(var Contact in Model)
                {
                    <div id="map">
                        <iframe src="@Contact.MapUrl" width="100%" height="500px" frameborder="0" style="border:0; border-radius: 10px; box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.15);" allowfullscreen=""></iframe>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="item phone">
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/phone-icon.png" alt="" style="max-width: 52px;">
                                <h6>@Contact.Phone<br><span>Telefon Numarası</span></h6>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="item email">
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/email-icon.png" alt="" style="max-width: 52px;">
                                <h6>@Contact.Email<br><span>Email Adresi</span></h6>
                            </div>
                        </div>
                    </div>
                }
                
            </div>
            @await Component.InvokeAsync("_UIDefaultContactMessageComponentPartial")
        </div>
    </div>
</div>
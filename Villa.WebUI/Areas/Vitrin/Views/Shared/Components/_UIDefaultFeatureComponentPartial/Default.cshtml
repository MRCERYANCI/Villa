@model List<ResultFeatureDto>


@foreach(var Features in Model)
{
    <div class="featured section">
        <div class="container">
            <div class="row">
                <div class="col-lg-4">
                    <div class="left-image">
                        <img src="@Features.ImageUrl" alt="">
                        <a href="property-details.html"><img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/featured-icon.png" alt="" style="max-width: 60px; padding: 0px;"></a>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="section-heading">
                        <h6>@Features.Title</h6>
                        <h2>@Features.SubTitle</h2>
                    </div>
                    @await Component.InvokeAsync("_UIDefaultQuestionComponentPartial")
                </div>
                <div class="col-lg-3">
                    <div class="info-table">
                        <ul>
                            <li>
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/info-icon-01.png" alt="" style="max-width: 52px;">
                                <h4>@Features.Square m2<br><span>Toplam Daire Alanı</span></h4>
                            </li>
                            <li>
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/info-icon-02.png" alt="" style="max-width: 52px;">
                                <h4>Kontrat Türü<br><span>@Features.Contract</span></h4>
                            </li>
                            <li>
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/info-icon-03.png" alt="" style="max-width: 52px;">
                                <h4>Ödeme Türü<br><span>@Features.Payment</span></h4>
                            </li>
                            <li>
                                <img src="~/Vitrin Temaları/VillaAgency-1.0.0/assets/images/info-icon-04.png" alt="" style="max-width: 52px;">
                                <h4>Güvenlik<br><span>@Features.Safety</span></h4>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
